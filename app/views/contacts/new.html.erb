<h1>New contact</h1>

<% form_for(@contact) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :id %><br />
    <%= f.text_field :id %>
  </p>
  <p>
    <%= f.label :contactid %><br />
    <%= f.text_field :contactid %>
  </p>
  <p>
    <%= f.label :labelnumber %><br />
    <%= f.text_field :labelnumber %>
  </p>
  <p>
    <%= f.label :labeldetails %><br />
    <%= f.text_field :labeldetails %>
  </p>
  <p>
    <%= f.label :tags %><br />
    <%= f.text_field :tags %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', contacts_path %>