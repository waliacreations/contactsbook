<h1>Editing contact</h1>

<% form_for(@contact) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :id %><br />
    <%= f.text_field :id %>
  </p>
  <p>
    <%= f.label :contactid %><br />
    <%= f.text_field :contactid %>
  </p>
  <p>
    <%= f.label :labelnumber %><br />
    <%= f.text_field :labelnumber %>
  </p>
  <p>
    <%= f.label :labeldetails %><br />
    <%= f.text_field :labeldetails %>
  </p>
  <p>
    <%= f.label :tags %><br />
    <%= f.text_field :tags %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @contact %> |
<%= link_to 'Back', {:action=>'show', :id=>@contact} %>

<br/>
<%=link_to 'ViewSummary',{:action=>'viewsummary',:uid=>params[:uid]}%>